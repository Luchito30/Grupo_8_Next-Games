<!DOCTYPE html>
<html lang="es">
     <!-- Bootstrap-->
     <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
     integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
<%- include('./partials/head.ejs') %>

    <body class="body__home" data-userId="<%= locals.userLogin?.id %>">
        <%- include('./partials/loader.ejs') %>
        <div class="header__boostrap">
            <%- include('./partials/header') %>
        </div>
                <main>

                    <%- include('./partials/bannerPrincipal.ejs') %>

                        <%- include('./partials/formadepago.ejs') %>

                            <div class="home__main--titulo">
                                <h2>ULTIMAS OFERTAS</h2><a href="/products/categorias/3"> ver más</a>
                            </div>
                            <section class="home__main__section">
                                <% inSale.forEach(({id,image,price,discount,name, state,isFavorite})=> { %>
                                    <%- include('./partials/product',{ id, image, price, discount, name,
                                        state,toThousand,isFavorite }) %>
                                        <% }) %>
                            </section>



                            <div class="home__main--titulo">
                                <h2>JUEGOS</h2> <a href="/products/subcategory/1"> ver más</a>
                            </div>

                            <section class="home__main__juegos">

                                <!-- Slider main container -->
                                <div class="swiper swiper-notebook1">
                                    <!-- Additional required wrapper -->
                                    <div class="swiper-wrapper">
                                        <!-- Slides -->
                                        <% juegos.forEach(({id,image,price,discount,name,state,isFavorite})=> { %>
                                            <%- include('./partials/carrusel',{ id, image, price, discount, name, state,isFavorite
                                                }) %>
                                                <% }) %>
                                    </div>

                                    <!-- If we need navigation buttons -->
                                    <div class="swiper-button-prev"></div>
                                    <div class="swiper-button-next"></div>

                                </div>
                            </section>

                            <%- include('./partials/bannermid.ejs') %>

                                <div class="home__main--titulo">
                                    <h2>NOTEBOOKS</h2> <a href="/products/subcategory/5"> ver más</a>
                                </div>

                                <section class="home__main__notebook">

                                    <!-- Slider main container -->
                                    <div class="swiper swiper-notebook1">
                                        <!-- Additional required wrapper -->
                                        <div class="swiper-wrapper">
                                            <!-- Slides -->
                                            <% computacion.forEach(({id,image,price,discount,name,state,isFavorite})=> { %>
                                                <%- include('./partials/carrusel',{ id, image, price, discount, name,
                                                    state,isFavorite }) %>
                                                    <% }) %>
                                        </div>
                                        <!-- If we need navigation buttons -->
                                        <div class="swiper-button-prev"></div>
                                        <div class="swiper-button-next"></div>

                                    </div>
                                </section>

                                <div class="home__main--titulo">
                                    <h2>NUEVOS INGRESOS</h2> <a href="/products/categorias/2"> ver más</a>
                                </div>

                                <section class="home__main__ingresos">

                                    <!-- Slider main container -->
                                    <div class="swiper swiper-notebook1">
                                        <!-- Additional required wrapper -->
                                        <div class="swiper-wrapper">
                                            <!-- Slides -->
                                            <% ingresos.forEach(({id,image,price,discount,name,state,isFavorite})=> { %>
                                                <%- include('./partials/carrusel',{ id, image, price, discount, name,
                                                    state,isFavorite }) %>
                                                    <% }) %>
                                        </div>
                                        <!-- If we need navigation buttons -->
                                        <div class="swiper-button-prev"></div>
                                        <div class="swiper-button-next"></div>

                                    </div>

                                </section>


                                <%- include('./partials/bannerfinal.ejs') %>

                                    <div class="home__main--titulo">
                                        <h2>TARJETAS DIGITALES PS4</h2><a href="/products/subcategory/3"> ver más</a>
                                    </div>


                                    <section class="home__main__tarjetasd">

                                        <!-- Slider main container -->
                                        <div class="swiper swiper-notebook1">
                                            <!-- Additional required wrapper -->
                                            <div class="swiper-wrapper">
                                                <!-- Slides -->
                                                <% tarjetas.forEach(({id,image,price,discount,name,state,isFavorite})=> { %>
                                                    <%- include('./partials/carrusel',{ id, image, price, discount,
                                                        name, state,isFavorite }) %>
                                                        <% }) %>
                                            </div>
                                            <!-- If we need navigation buttons -->
                                            <div class="swiper-button-prev"></div>
                                            <div class="swiper-button-next"></div>
                                        </div>
                                    </section>

                                    <div class="home__main--titulo">
                                        <h2>CONSOLAS</h2><a href="/products/subcategory/4"> ver más</a>
                                    </div>


                                    <section class="home__main__ps4">

                                        <!-- Slider main container -->
                                        <div class="swiper swiper-notebook1">
                                            <!-- Additional required wrapper -->
                                            <div class="swiper-wrapper">
                                                <!-- Slides -->
                                                <% consolas.forEach(({id,image,price,discount,name,state,isFavorite})=> { %>
                                                    <%- include('./partials/carrusel',{ id, image, price, discount,
                                                        name, state,isFavorite }) %>
                                                        <% }) %>
                                            </div>
                                            <!-- If we need navigation buttons -->
                                            <div class="swiper-button-prev"></div>
                                            <div class="swiper-button-next"></div>

                                        </div>


                                    </section>
                </main>
                <!-- carrito -->
                <%- include('./partials/carrito.ejs') %>
                    <!--whatsapp-->
                    <%- include('./partials/whatsApp.ejs') %>
                        <!-- footer -->
                        <div id="profile--footer">
                            <%- include('./partials/footer.ejs') %>
                          </div>
              
                            <!--swiper  -->
                            <script src="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.js"></script>

                            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
                            integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
                            crossorigin="anonymous"></script>

                            <!-- slider -->
                            <script src="/javascripts/notebook.js"></script>

                            <!-- fixed -->
                            <script src="/javascripts/fixed.js"></script>
                            <script src="/javascripts/favoritesHome.js"></script>
                            <script src="/javascripts/loader.js"></script>
                            <script src="/javascripts/addproductToCard.js"></script>

    </body>

</html>